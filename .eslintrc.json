{
  "parser": "babel-eslint",
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier", "react", "jest"],
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-console": 0,
    "no-debugger": 0
  },
  "globals": {
    "constants": true,
    "functions": true,
    "require": true
  }
}
